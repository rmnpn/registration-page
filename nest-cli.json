{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      "@nestjs/swagger"
    ],
    "assets": [
      {
        "include": "modules/hbs-engine/views",
        "watchAssets": true,
        "outDir": "dist/src"
      }
    ]
  }
}
